{
  "expo": {
    "name": "myLocale",
    "slug": "myLocale",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#FFF9FA",
    "icon": "./assets/images/dark.png",
    "scheme": "mylocale",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFF9FA"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
      "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow myLocale to access your location."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0F1108"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
